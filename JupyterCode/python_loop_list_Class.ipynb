   "source": [
    "#write code to print this pattern\n",
    "# 5 4 3 2 1 \n",
    "# 4 3 2 1 \n",
    "# 3 2 1 \n",
    "# 2 1 \n",
    "# 1\n",
    "\n",
    "i=5\n",
    "j=5\n",
    "while(j>=1):\n",
    "    while(i<=j):\n",
    "        print(i,end=\" \")\n",
    "        if i==1:\n",
    "            break\n",
    "        i-=1\n",
    "    j-=1"
